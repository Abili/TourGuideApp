<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.raisac.tourguideapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/crestecrane"
        android:label="@string/app_name"
        android:roundIcon="@drawable/crestecrane"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RestuarantDetailsActivity"
            android:label="Restuarants"/>
        <activity android:name=".AttractionsDetailsActivity"
            android:label="Attractions"/>
        <activity android:name=".PublicPlacesDetailsActivity"
            android:label="Public Places"/>
        <activity android:name=".Home"
            android:label="Home"/>
        <activity android:name=".InsideKampala"
            android:label="Inside Kampala"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>